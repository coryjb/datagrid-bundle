{
    "name": "coryjb/datagrid-bundle",
    "description": "This bundle provides a convenient way to show datagrids in Twig",
    "keywords": ["datagrid"],
    "type": "symfony-bundle",
    "license": "MIT",
	"homepage": "https://github.com/coryjb/datagrid-bundle",
    "authors": [
        {
            "name": "Cory Becker",
            "email": "cory@bws.io",
			"homepage": "http://github.com/coryjb/"
        }
    ],
    "require": {
		"php": ">=5.3.2",
        "symfony/symfony": ">=2.0.0",
        "doctrine/common": "~2.2",
		"twig/twig": ">=1.5.0"
    },
    "autoload": {
        "psr-0": { "Becker\\GridBundle": "" }
    },
	"target-dir": "Becker/GridBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
